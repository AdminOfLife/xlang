#!/usr/bin/xlang

void main()
{
	j=1;
	print "This main definition!";
	while (j < 10)
	{
		print j;
		j+=1;
		if (j>5)
		{
			break;
			print "before continue";
			continue;
			print "after continue";
		}
		print "After break";
	}
	print "after while";
	print""; print"";
}

void fucntion2(int a, int b, int c)
{
	j=1;
