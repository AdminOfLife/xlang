#!/usr/bin/xlang.exe
int test_while()
{
	print "Check while statement...";

	i = 1;
	while(i < 10)
	{
		print "This is while operator!";
		if ((i >= 5) && (i <=7))
		{
			print "(( " i " >= 5) && (" i " <=7))";
			j=1;
			while (j < 10)
			{
				print j;
				if (j>5)
				{
					break;
					continue;
				}
				j+=1;
			}
		}
		i+=1;
	}
	print "After while";
	print "End check";
}
